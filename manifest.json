{
    "name": "DocString",
    "description": "Easily access any API documentation straight from the omnibox",
    "version": "0.0.1",
    "manifest_version": 2,
    "icons": {
        "16": "icons/logo16.png",
        "32": "icons/logo32.png",
        "48": "icons/logo64.png",
        "128": "icons/logo128.png"
    },
    "omnibox": {
        "keyword": "doc"
    },
    "background": {
        "persistent": false,
        "scripts": ["bundle.js"]
      }
}